{
  "name": "Facebook Turtle",
  "version": "1.0",
  "content_scripts": [{
      "matches": ["*://*.facebook.com/*"],
      "js": ["jquery.min.js", "turtle.js"],
      "run_at": "document_start"
  }],
  "browser_action": {
    "name": "Kill Facebook's News Feed",
    "icons": []
  },
  "manifest_version": 2
}